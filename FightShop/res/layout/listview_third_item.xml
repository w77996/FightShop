<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >
    <ImageView
        android:id="@+id/iv_third_listvimg"
        android:background="@drawable/ic_launcher"
		android:layout_width="50dp"
		android:layout_height="50dp"
		android:layout_margin="5dp"
        />
    	<TextView
    	android:id="@+id/tv_third_listname"
        android:layout_toRightOf="@+id/iv_third_listvimg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="19sp"
        android:text="name"
        />
       <TextView
        android:id="@+id/tv_third_listnum"
        android:layout_alignBottom="@+id/iv_third_listvimg"
        android:layout_below="@+id/tv_third_listname"
        android:layout_toRightOf="@+id/iv_third_listvimg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:text="num"
        />
        

</RelativeLayout>
